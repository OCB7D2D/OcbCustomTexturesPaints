<configs>
	<append xpath="/paints">
		<opaque id="test_square" name="txName_test_colors_2x2" x="0" y="0" w="2" h="2" blockw="2" blockh="2">
			<!-- Diffuse must not have an alpha channel or texture copy will fail! -->
			<property name="Diffuse" value="512:512:1:1:0,512:512:0:1:1,512:512:1:0:0,512:512:0:0:1"/>
			<!-- Specular has the format: Metallic=R, Occlusion=G, Emission=B, Roughness=A -->
			<property name="Specular" value="512:512:0.1:1:0:0.85,512:512:0.1:1:0:0.85,512:512:0.1:1:0:0.85,512:512:0.1:1:0:0.85"/>
			<!-- Although normal maps are missing, we will fill in and use neutral/flat one -->
			<property name="Group" value="txGroupCustom"/>
			<property name="SortIndex" value="1"/>
			<property name="PaintCost" value="4"/>
			<property name="GlobalUV" value="False"/>
			<property name="Color" value="0.48,0.35,0.29"/>
			<property name="Material" value="wood"/>
		</opaque>

